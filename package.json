{
  "name": "workspace-smart-search",
  "publisher": "alonpe",
  "displayName": "Workspace Smart Search",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.1"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "intersystems-utils.targetedOpen",
        "title": "Open InterSystems Document..."
      }
    ],
    "keybindings": [
      {
        "command": "intersystems-utils.targetedOpen",
        "key": "ctrl+alt+o",
        "when": "vscode-objectscript.connectActive"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.1",
    "@types/node": "22.x",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlonPeleg/workspace-smart-search"
  }
}